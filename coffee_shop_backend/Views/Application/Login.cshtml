@model LoginViewModel

<a href="@Url.Action("Register","Application")">申請</a>
<a href="@Url.Action("Settings","Application")">設定</a>

<form method="post" asp-controller="ControllerName" asp-action="ActionName" novalidate>
    <div class="form-group">
        <label asp-for="Account">姓名</label>
        <input asp-for="Account" class="form-control" />
        <span asp-validation-for="Account" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Password"></label>
        <input asp-for="Password" class="form-control" type="password" />
        <span asp-validation-for="Password" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">提交</button>
</form>
