@model IEnumerable<RecordListViewModel>
@{
  
}

@section Scripts
{

}


<h1>問卷調查-查詢</h1>

<table class="table table-striped table-bordered table-hover" id="dataTable" width="100%" cellspacing="0">
    <thead>
        <tr>
            <th>姓名</th>
            <th>問卷類別</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Any()){
            foreach (RecordListViewModel item in Model)
            {
                <tr>
                    <td>@item.RegName</td>
                    <td><a href="@Url.Action("Record","QuestionnaireFront", new {regId = item.RegId, applicationId = item.ApplicationId})">@item.ApplicationName</a></td>
                </tr>
            }
        }
    </tbody>
</table>